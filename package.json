{
  "name": "grakchawwaa-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Discord example app",
  "main": "src/index.ts",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "setupLocalDB": "dotenvx run -f .env.dev -- ts-node infra/setupLocalDB.ts",
    "setupHerokuDB": "dotenvx run -f .env.prod -- ts-node infra/setupHerokuDB.ts",
    "insertTestData": "dotenvx run -f .env.dev -- ts-node infra/insertTestData.ts",
    "dev": "dotenvx run -f .env.dev -- ts-node src/index.ts",
    "prod": "dotenvx run -f .env.prod -- ts-node src/index.ts",
    "reset-commands-dev": "dotenvx run -f .env.dev -- ts-node infra/reset-commands.ts",
    "reset-commands-prod": "dotenvx run -f .env.prod -- ts-node infra/reset-commands.ts",
    "see-heroku-log": "heroku logs --tail --app grakchawwaa --dyno=worker.1",
    "lint": "eslint '**/*.ts'",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci"
  },
  "author": "ExeAy",
  "license": "MIT",
  "dependencies": {
    "@sapphire/discord.js-utilities": "^7.3.2",
    "@sapphire/framework": "^5.4.0",
    "@sapphire/pieces": "^4.4.1",
    "@swgoh-utils/comlink": "^1.4.1",
    "discord.js": "~14.24.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "heroku": "^10.15.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
